{
  "name": "johannesfoto",
  "description": "A Material Design theme, custom made for the Johannes Foto Blog.",
  "keywords": [
    "ghost-theme"
  ],
  "version": "2.1.0",
  "scripts": {
    "build": "gulp bundle"
  },
  "license": "MIT",
  "author": {
    "name": "Jan Heinrich Reimer",
    "email": "jan.heinrich.reimer@reimer.dev",
    "url": "https://reimer.dev"
  },
  "config": {
    "posts_per_page": 10,
    "card_assets": true,
    "image_sizes": {
      "xxs": {
        "width": 30
      },
      "xs": {
        "width": 100
      },
      "s": {
        "width": 300
      },
      "m": {
        "width": 600
      },
      "l": {
        "width": 1000
      },
      "xl": {
        "width": 2000
      }
    }
  },
  "browserslist": [
    "defaults"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@types/jquery": "^3.5.30",
    "@types/material-design-lite": "^1.1.22",
    "@types/prismjs": "^1.26.4",
    "baguettebox.js": "^1.12.0",
    "exif-js": "^2.3.0",
    "jquery": "^3.7.1",
    "material-design-lite": "^1.3.0",
    "prismjs": "^1.21.0",
    "sass-material-colors": "^0.0.5"
  },
  "devDependencies": {
    "@types/gulp": "^4.0.17",
    "@types/gulp-autoprefixer": "^0.0.37",
    "@types/gulp-csso": "^4.0.5",
    "@types/gulp-dart-sass": "^1.0.5",
    "@types/gulp-rename": "^2.0.6",
    "@types/gulp-uglify": "^3.0.11",
    "@types/gulp-zip": "^4.0.4",
    "@types/node-sass": "^4.11.7",
    "@types/webpack": "^5.28.5",
    "@types/webpack-stream": "^3.2.15",
    "bestzip": "^2.1.7",
    "del": "^7.1.0",
    "gscan": "^4.43.1",
    "gulp": "^5.0.0",
    "gulp-autoprefixer": "^9.0.0",
    "gulp-csso": "^4.0.1",
    "gulp-dart-sass": "^1.1.0",
    "gulp-rename": "^2.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-zip": "^6.0.0",
    "webpack": "^5.92.1",
    "webpack-stream": "^7.0.0"
  }
